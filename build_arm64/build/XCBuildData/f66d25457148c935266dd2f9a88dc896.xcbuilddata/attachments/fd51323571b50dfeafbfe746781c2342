#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64
  make -f /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64
  make -f /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64
  make -f /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64
  make -f /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/CMakeScripts/ReRunCMake.make
fi

