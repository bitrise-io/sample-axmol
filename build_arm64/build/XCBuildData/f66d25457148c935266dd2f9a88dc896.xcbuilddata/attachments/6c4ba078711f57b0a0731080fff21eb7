#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/tests/cpp-tests
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/tests/cpp-tests/Source/shaders/simple_shadow.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/custom/simple_shadow_vs
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/tests/cpp-tests
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/tests/cpp-tests/Source/shaders/simple_shadow.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/custom/simple_shadow_vs
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/tests/cpp-tests
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/tests/cpp-tests/Source/shaders/simple_shadow.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/custom/simple_shadow_vs
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/tests/cpp-tests
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/tests/cpp-tests/Source/shaders/simple_shadow.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/custom/simple_shadow_vs
fi

