#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/quadColor.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/quadColor_vs
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/quadColor.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/quadColor_vs
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/quadColor.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/quadColor_vs
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/quadColor.vert --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/quadColor_vs
fi

