#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL,MAX_DIRECTIONAL_LIGHT_NUM=1,MAX_POINT_LIGHT_NUM=1,MAX_SPOT_LIGHT_NUM=1" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --frag=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/colorNormal.frag --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/colorNormal_fs
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL,MAX_DIRECTIONAL_LIGHT_NUM=1,MAX_POINT_LIGHT_NUM=1,MAX_SPOT_LIGHT_NUM=1" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --frag=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/colorNormal.frag --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/colorNormal_fs
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL,MAX_DIRECTIONAL_LIGHT_NUM=1,MAX_POINT_LIGHT_NUM=1,MAX_SPOT_LIGHT_NUM=1" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --frag=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/colorNormal.frag --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/colorNormal_fs
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/engine/axmol/core
  /Users/lpusok/Develop/xsample/axmol/axmol/tools/external/glslcc/glslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL,MAX_DIRECTIONAL_LIGHT_NUM=1,MAX_POINT_LIGHT_NUM=1,MAX_SPOT_LIGHT_NUM=1" --include-dirs=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders --sgs --reflect --frag=/Users/lpusok/Develop/xsample/axmol/axmol/core/renderer/shaders/colorNormal.frag --output=/Users/lpusok/Develop/xsample/axmol/axmol/build_arm64/runtime/axslc/colorNormal_fs
fi

